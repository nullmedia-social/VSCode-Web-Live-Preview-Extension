{
  "name": "web-live-preview",
  "displayName": "Web Live Preview",
  "description": "Multi-file Live Preview extension for vscode.dev",
  "version": "0.0.3",
  "publisher": "kingnullboy",
  "engines": { "vscode": "^1.78.0" },
  "browser": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kingnullboy/VSCode-Web-Live-Preview-Extension.git"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:livePreview.open"],
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "@types/vscode": "^1.78.0"
  },
  "contributes": {
    "commands": [
      { "command": "livePreview.open", "title": "Live Preview: Open" }
    ]
  }
}