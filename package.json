{
  "name": "web-live-preview",
  "displayName": "Web Live Preview",
  "description": "Multi-file Live Preview extension for vscode.dev",
  "version": "0.0.1",
  "publisher": "kingnullboy",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:livePreview.open"],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "vscode": "^1.90.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "livePreview.open",
        "title": "Live Preview: Open"
      }
    ]
  }
}