{
  "name": "web-live-preview",
  "displayName": "Web Live Preview",
  "description": "Multi-file Live Preview extension for vscode.dev",
  "version": "0.0.1",
  "publisher": "kingnullboy",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:livePreview.open"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "@types/node": "^24.3.0",
    "@types/vscode": "^1.78.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "livePreview.open",
        "title": "Live Preview: Open"
      }
    ]
  }
}